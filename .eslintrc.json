{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "node": true
  },
  "rules": {
    "import/no-extraneous-dependencies": [2, {"devDependencies": ["gulpfile.babel.js"]}],
    "no-unused-expressions": 0,
    "quotes": [2, "single"],
    "padded-blocks": [0],
    "eol-last": [0],
    "no-underscore-dangle": [0],
    "max-len": [0],
    "no-unused-vars": [0],
    "class-methods-use-this": [0],
    "no-console": [0],
    "comma-dangle": [2, "never"],
    "no-param-reassign": [2, { "props": false }],
    "prefer-destructuring": [0],
    "indent": [2, 2, { "MemberExpression": "off", "CallExpression": {"arguments": "first"} }]
  }
}